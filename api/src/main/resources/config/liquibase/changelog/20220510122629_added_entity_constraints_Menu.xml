<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Menu.
    -->
    <changeSet id="20220510122629-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="menu_id"
                                 baseTableName="rel_menu__restaurant"
                                 constraintName="fk_rel_menu__restaurant__menu_id"
                                 referencedColumnNames="id"
                                 referencedTableName="menu"/>

        <addForeignKeyConstraint baseColumnNames="restaurant_id"
                                 baseTableName="rel_menu__restaurant"
                                 constraintName="fk_rel_menu__restaurant__restaurant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="restaurant"/>

        <addForeignKeyConstraint baseColumnNames="categorie_id"
                                 baseTableName="menu"
                                 constraintName="fk_menu__categorie_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categorie"/>
    </changeSet>
</databaseChangeLog>
